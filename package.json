{
  "name": "android-appium-ts",
  "version": "1.0.0",
"scripts": {
  "wdio": "wdio run ./wdio.conf.ts",
  "clean": "rimraf allure-results allure-report",
  "allure:generate": "allure generate allure-results --clean -o allure-report",
  "allure:open": "allure open allure-report",
  "test:local": "npm run clean && .\\run-tests.ps1 -Headless",
  "test:ci": "npm run clean && npm run wdio && npm run allure:generate"
},
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/node": "^20.5.7",
    "@wdio/allure-reporter": "^9.24.0",
    "@wdio/appium-service": "^9.23.3",
    "@wdio/cli": "^9.23.3",
    "@wdio/local-runner": "^9.23.3",
    "@wdio/mocha-framework": "^9.23.3",
    "@wdio/spec-reporter": "^9.23.3",
    "@wdio/types": "^9.23.3",
    "allure-commandline": "^2.36.0",
    "appium": "^3.2.0",
    "appium-uiautomator2-driver": "^4.2.9",
    "chai": "^6.2.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
